                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT        Sales.OrderDetails.orderid, Sales.OrderDetails.productid, Sales.OrderDetails.unitprice, Sales.OrderDetails.qty, Sales.OrderDetails.discount,Sales.Orders.custid, Sales.Orders.empid, 
