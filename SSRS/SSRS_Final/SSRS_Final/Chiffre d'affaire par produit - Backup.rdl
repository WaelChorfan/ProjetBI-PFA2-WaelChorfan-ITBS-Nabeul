      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        dimProduits.idDimProd, dimProduits.nomProduit, SUM(FACT_CMD.CaCde) AS CaProduit
FROM            dimProduits INNER JOIN
                         FACT_CMD ON dimProduits.idDimProd = FACT_CMD.idDimProd
GROUP BY dimProduits.idDimProd, dimProduits.nomProduit</CommandText>
        <Timeout>0</Timeout>
      </Query>
